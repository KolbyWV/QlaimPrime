{
  "name": "qlaim",
  "version": "1.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --port 8081",
    "android": "expo start --android --port 8081",
    "ios": "expo start --ios --port 8081",
    "web": "expo start --web --port 8081"
  },
  "dependencies": {
    "@apollo/client": "^4.1.6",
    "@react-navigation/bottom-tabs": "^7.14.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.13.0",
    "expo": "^54.0.33",
    "expo-secure-store": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "graphql": "^16.12.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4"
  },
  "expo": {
    "plugins": [
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow Qlaim to access your Face ID biometric data.",
          "touchIDPermission": "Allow Qlaim to access your Touch ID biometric data."
        }
      ]
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ]
  },
  "private": true
}
